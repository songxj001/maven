<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<settings>
    	<setting name="logImpl" value="LOG4J"/>
	</settings>
	<!-- 实体类,简称 -设置别名 -->
	<typeAliases>
		<!-- 导航树 -->
		<typeAlias alias="treeBean" type="com.jk.model.tree.TreeBean" />
		<!-- 部门 -->
		<typeAlias alias="deptBean" type="com.jk.model.user.DeptBean" />
		<!-- 用户 -->
		<typeAlias alias="userBean" type="com.jk.model.user.UserBean" />
		<!-- 角色 -->
		<typeAlias alias="roleBean" type="com.jk.model.user.RoleBean" />
		<!-- 权限树 -->
		<typeAlias alias="powerTreeBean" type="com.jk.model.tree.PowerTreeBean" />
		<!-- 角色权限关联 -->
		<typeAlias alias="rolePowerBean" type="com.jk.model.user.RolePowerBean" />
		<!-- 权限菜单 -->
		<typeAlias alias="menuBean" type="com.jk.model.user.MenuBean" />
		<!-- 登陆日志 -->
		<typeAlias alias="loginHistoryBean" type="com.jk.model.user.LoginHistoryBean" />
		<!-- 文件资源 -->
		<typeAlias alias="fileBean" type="com.jk.model.user.FileBean" />
		
	</typeAliases>
	<!-- 实体接口映射资源 -->
	<!--
		说明：如果xxMapper.xml配置文件放在和xxMapper.java统一目录下，mappers也可以省略，因为org.mybatis.spring.mapper.MapperFactoryBean默认会去查找与xxMapper.java相同目录和名称的xxMapper.xml
	-->
	<!-- <mappers>
		<mapper resource="com/jk/mapper/user/userMapper.xml" />
	</mappers> -->
</configuration>  